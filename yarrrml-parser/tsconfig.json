{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2019"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./bin",
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016"
  }
}
